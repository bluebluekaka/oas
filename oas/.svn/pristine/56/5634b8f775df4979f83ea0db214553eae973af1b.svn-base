   function getReplace(str,ochar,nchar){ //ochar =  /\./g  == .
		while(str.indexOf(ochar)!=-1)//寻找每一个中文逗号，并替换
		{
			str=str.replace(ochar,nchar);
			 
		}
		return str;
 	}
