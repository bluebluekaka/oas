    function f_endOrder(){
		if(checkedCustomer.length==0){
			 $.ligerDialog.warn('<br/>请选择一个订单');
			 return ;	
		} 
		 	var rows = grid.getCheckedRows();
			var str = "";
           	 $(rows).each(function ()
            {
            	var s = this;
            	var i = 1;
                for(var n in s){  
		           	if(i==5){
		           		if(s[n]!='&#20986;&#31080;&#25104;&#21151;'){
		           			str = 'err';
		           			break;
		           		}
		           	}else if(i==2){
		           		if(s[n]!='false'){
		           			str = 'err1';
		           			break;
		           		}
		           	}
		           	if(i==3||i==13){
		           		if(i==13) str += s[n]+";";
		           		else str += s[n]+",";
		           	}
					
		           	i++;
	      		} 
            });
           	if(str!='err'){
				if(str.indexOf('err1')!=-1){
					alert("您选择了还未到审核时间的订单。");
					}else{
	            $.ligerDialog.confirm('<br/>确定所选订单通过审核吗？',
	                 	function (yes) {
		                 	if(yes==true) {
		                 		location.href = "<%=action%>?action=opass&sid="+str;  
		                 	}
	                 	} 
	            );}
        	}else{
        		alert("您选择了不能一键审核的订单。");
        	}
    }
   
